{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "flex",
      "camelcase": "Flex",
      "title": "flex",
      "scope": "source.flex",
      "file-types": [
        "l"
      ],
      "injection-regex": "^flex$",
      "class-name": "TreeSitterFlex"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "a tree-sitter grammar for flex",
    "authors": [
      {
        "name": "may"
      }
    ],
    "links": {
      "repository": "https://github.com/m4rh3n1ng/flex"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
