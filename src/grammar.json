{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "flex",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section1"
        },
        {
          "type": "SYMBOL",
          "name": "section2"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "trailing_code"
              },
              "named": true,
              "value": "embedded_code"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "[ \\t\\r\\v\\f]+"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "_space": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "(\\p{XID_Start}|%)\\p{XID_Continue}*"
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "block_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "([^*]|\\*[^/])*"
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "_comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "block_comment"
        }
      ]
    },
    "prologue": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%{"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "prologue_body"
          },
          "named": true,
          "value": "embedded_code"
        },
        {
          "type": "STRING",
          "value": "%}"
        }
      ]
    },
    "prologue_body": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "([^%]|%[^}])*"
      }
    },
    "alias": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "rule"
        }
      ]
    },
    "section1": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "prologue"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "alias"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_whitespace"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_comment"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "_space"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%%"
        }
      ]
    },
    "rule": {
      "type": "PATTERN",
      "value": "[^\"]\\S*?"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^\"]*"
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "embedded_code": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^}]+"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string"
            },
            {
              "type": "SYMBOL",
              "name": "rule"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_whitespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "embedded_code"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_comment"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_newline"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "section2": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "declaration"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%%"
        }
      ]
    },
    "trailing_code": {
      "type": "PATTERN",
      "value": "(.|\\n)+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}